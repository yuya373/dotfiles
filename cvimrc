set noautofocus
let locale = "jp"
let mapleader = ","
let searchalias g = "google" 
map M centerMatchH
map T centerMatchT
map B centerMatchB
map <Leader>f createTabbedHint
map <Leader>h createHoverHint
map <Leader>H createUnHoverHint
map tt :tabnew<Space>
map tn nextTab
map tp previousTab
map tf firstTab
map tl lastTab
map b :buffer<Space>
imap <C-w> deleteWord
imap <C-h> deleteChar
imap <C-b> backwardWord
imap <C-f> forwardWord
