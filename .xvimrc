set wrap
set wrapscan
set ignorecase
set smartcase
set incsearch
set hlsearch
set number
set clipboard=unnamed
set guioptions=r
nmap <silent> j gj
nmap <silent> gj j
nmap <silent> k gk
nmap <silent> gk k
nmap <silent> $ g$
nmap <silent> g$ $
vmap <silent> j gj
vmap <silent> gj j
vmap <silent> k gk
vmap <silent> gk k
vmap <silent> $ g$
vmap <silent> g$ $
nmap <C-j> <C-w>j
nmap <C-k> <C-w>k
nmap <C-h> <C-w>h
nmap <C-l> <C-w>l
nmap <C-c> <C-w>c
nmap <C-a> <C-w>n
