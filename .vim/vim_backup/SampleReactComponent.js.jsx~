/** @jsx React.DOM */
define(["react", "core/component"], function(React, component) {
  return React.createClass({
    mixins: [component],
    sample: function (e) {
      var t = e.currentTarget
      $(t).html("SampleReactComponent Clicked")
      console.log("clicked");
    },
    render: function () {
      var sample
      sample = $('#sampleReactComponent')
      sample.bind("click", this.sample)
      return (
        <div className="well">
          SampleComponent installed
        </div>
      );
    }
  });
});
