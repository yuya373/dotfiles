function zenToHan (id){
  var zip = document.getElementById(id);
  console.log(zip);
  zip.value = zenToHanStr(zip.value);
};

function zenToHanStr(str){
  function toHan(match){
    return String.fromCharCode(match.charCodeAt(0) - 0xFEE0);
  }
  return str.replace(/[０-９]/g, toHan);
};

function changeToHan (el){
  var val = el.val()
}
