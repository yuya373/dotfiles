/** @jsx React.DOM */

define([
    "react",
    "components/mixins/ListingAvailabilityAppMixin"
  ], function(React, ListingAvailabilityAppMixin) {
  // ListingAvailabilityAppMixin = _.omit(ListingAvailabilityAppMixin, 'makePrimaryButton')

  var ListingStatusApp = React.createClass({
    mixins: [ListingAvailabilityAppMixin],
    getInitialState: function() {
      return {
        "available"  : null,
        "totalPrice" : null,
        "loading"    : false
        "nobutton"
      };
    }
  });
  return ListingStatusApp;
});
