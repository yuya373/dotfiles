function countUp(){
  function (){
    var n = 1;
    return function (){
      n += 1
      console.log(n)
    }
  }
};

countUp()


console.log("--------------------------------")
var module = function () {
  var n = 1;
  return function () {
      n += 1;
      console.log(n)
    }
}();

module()
module()
module()
