/** @jsx React.DOM */

define([
    "react",
    "components/mixins/ListingAvailabilityAppMixin"
  ], function(React, ListingAvailabilityAppMixin) {
  var ListingAvailabilityApp = React.createClass({
    mixins: [ListingAvailabilityAppMixin],
    getInitialState: function() {
      return {
        "available"  : null,
        "totalPrice" : null,
        "loading"    : false,
        "nobutton"   : true
      };
    }
  });
  return ListingAvailabilityApp;
});
