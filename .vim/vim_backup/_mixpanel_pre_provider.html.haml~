%script
  - unless Rails.env.production?
    mixpanel.disable();
  - pre_provider = PreProviderContact.find params[:id] if params[:id]
  - if pre_provider.present?
    mixpanel.alias("#{pre_provider.id}");
    mixpanel.track("pre_provider_contact");
    mixpanel.people.set({
    "$email": "#{pre_provider.email}",
    "$created": "#{pre_provider.created_at}",
    "$name": "#{pre_provider.company}",
    "$person": "#{pre_provider.person}",
    "$tel": "#{pre_provider.tel}",
    "$content": "#{pre_provider.content}",
    "credits": 0});
