` /*** @jsx React.DOM ***/ `

CommentList = React.createClass {
  render: ->
    commentNodes = @.props.data.map( (comment)->
      return `( <Comment author={comment.author}>{comment.text}</Comment> )`
    )
    return `(
      <div className="commentList">
        {commentNodes}
      </div>
    )`
}
