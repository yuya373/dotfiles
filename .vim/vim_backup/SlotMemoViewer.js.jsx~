/** @jsx React.DOM*/

define(["react"], function(React) {
  var SlotMemoViewer;
  SlotMemoViewer = React.createClass({
    getInitialState: function() {
      return {
        "hidden": true,
        "style": {},
        "start_at": null,
        "end_at": null,
        "date": null
      };
    },
    render: function() {
      console.log("SlotMemoViewer rendered", this);
      var contentStyle;
      // if(this.state.hidden === true) { return <div></div> };
      contentStyle = {
        "display": "block"
      };
      return (
        <div className="slot-prompt-shell">
          <div className="slot-memo-viewer">aaa</div>
        </div>
      );
      // return (
      // <div className="slot-prompt-shell" style={this.state.style} >
          // <div className="slot-price-viewer mod mod-default mod-round" style={contentStyle} >
            // <div className="viewer-header">
              // 料金
            // </div>
            // <div className="inner-mod">
              // {this.renderContent()}
            // </div>
          // </div>
        // </div>
    // );
    },
    renderContent: function() {
      return (
      <table className="table table-bordered table-fit table-small">
        <tr>
          <th> test </th>
          <td> test </td>
        </tr>
      </table>
      );
    },
    show: function(params) {
      this.setState({
        "hidden": false,
        "start_at": params.start_at,
        "end_at": params.end_at,
        "date": params.date,
        "style": params.style
      });
      console.log("ShowMemo#show");
      console.log(this);
    },
    dismiss: function () {
      this.setState({
        "hidden" : true
      })
      $(this.getDOMNode()).unbind("clickoutside");
    }
  });
  return SlotMemoViewer;
});
