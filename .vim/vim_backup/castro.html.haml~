.container
  %h1 Castro Examples

  %h2 Install JS Component

  %h4 Codes
  %p.well
    $castro.add_js_component("testComponent", "TestComponent")

  %h4 Outputs
  %p.well{:id=>"testComponent"}
  - $castro.add_js_component("testComponent", "TestComponent", {"dataKey"=>"dataValue"}, {"propsKey"=>"propsValue"})

  %h4 Sample
  %p.well{id: "sampleComponent"}
  - $castro.add_js_component("sampleComponent", "SampleComponent")

  %h2 Install React.js Component

  %h4 Codes
  %p.well
    $castro.add_react_component("reactComponent", "TestReactComponent")

  %h4 Outputs
  %p.well{:id=>"reactComponent"}
  - $castro.add_react_component("reactComponent", "TestReactComponent", {"dataKey"=>"dataValue"}, {"propsKey"=>"propsValue"})

  %h4 Sample
  -# %p.well{id: "sampleReactComponent"}
  - $castro.add_react_component("sampleReactComponent", "SampleReactComponent")
